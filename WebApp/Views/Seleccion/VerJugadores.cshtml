@model Dominio.Models.Seleccion

<h1>Lista de Jugadores</h1>

<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            País
        </dt>
        <dd class="col-sm-10">
            @Model.Pais.Nombre
        </dd>
        <dt class="col-sm-2">
            Jugadores
        </dt>
        <dd class="col-sm-10">

            @foreach (var item in Model.GetJugadoresDeSeleccion())
            {
                <tr>
                    <td>
                        @item.NombreCompleto  -
                    </td>

                </tr>

            }

        </dd>

    </dl>
</div>

<div>
    @*<a asp-action="Index">Volver a la lista</a>*@
</div>
